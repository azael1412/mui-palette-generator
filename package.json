{
  "name": "mui-palette-generator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A Material-UI theme generator with real-time preview and export functionality",
  "keywords": [
    "material-ui",
    "mui",
    "theme-generator",
    "react",
    "color-picker"
  ],
  "author": "Azael Espinosa <azael_1412@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/azael1412/mui-palette-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azael1412/mui-palette-generator.git"
  },
  "bugs": {
    "url": "https://github.com/azael1412/mui-palette-generator/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "chroma-js": "^3.1.2",
    "file-saver": "^2.0.5",
    "react": "^19.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/chroma-js": "^3.1.1",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.0.10",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "lint-staged": {
    "**/*.{jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
